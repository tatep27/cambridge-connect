    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.1.3",
    "jsdom": "^27.1.0",
    "postcss": "^8.4.47",
    "prisma": "^6.18.0",
